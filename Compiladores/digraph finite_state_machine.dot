        0 -> 7 [ label = "&epsilon;" ];
        1 -> 2 [ label = "c" ];
        2 -> 3 [ label = "d" ];
        3 -> 4 [ label = "&epsilon;" ];
        4 -> 5 [ label = "a" ];
        5 -> 6 [ label = "b" ];
        7 -> 8 [ label = "a" ];
        8 -> 9 [ label = "b" ];
        9 -> 4 [ label = "&epsilon;" ];
